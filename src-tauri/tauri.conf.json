{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-multiwindow-webkitgtk-bug",
  "version": "0.1.0",
  "identifier": "com.tauri-multiwindow-webkitgtk-bug.app",
  "build": {
    "beforeDevCommand": "yarn start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist/tauri-multiwindow-webkitgtk-bug/browser"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-multiwindow-webkitgtk-bug",
        "width": 600,
        "height": 500
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
